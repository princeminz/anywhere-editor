{
  "name": "Anywhere Editor",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://codeforces.com/problemset/problem/*",
        "https://codeforces.com/contest/*/problem/*",
        "https://cses.fi/problemset/task/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "manifest_version": 2,
  "description": "Write, edit, and compile code on numerous Competitive Programming websites without having to leave the website.",
  "author": "Samarpit Minz",
  "web_accessible_resources": [
    "*.js",
    "*.ttf",
    "*.js.map"
  ]
}